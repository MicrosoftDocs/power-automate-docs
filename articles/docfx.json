{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-MSPowerAutomate",
      "contributors_to_exclude": [
        "buck1ey",
        "bishalgoswami",
        "kathleenmcgrath",
        "marycc",
        "tinaburden",
        "almostdunh",
        "AshleyWoodruff",
        "kimberleybrown",
        "nathlaroche"
      ],
      "breadcrumb_path": "breadcrumb/toc.yml",
      "titleSuffix": "Power Automate",
      "feedback_system": "Standard",  
      "feedback_product_url": "https://ideas.powerautomate.com/",
      "search.app": [
        "Flow",
        "powerautomate-docs"
      ],
      "toc_preview": true,
      "recommendations": true
    },
    "fileMetadata": {
      "featureFlags": {
        "**/*.md": [
          "show_learn_banner"
        ]
      },
      "learn_banner_products": {
        "**/*.md": [
          "flow"
        ]
      },
      "searchScope": {
        "**.*": [
          "Power Platform",
          "Power Automate"
        ]
      },
      "ms.service": {
        "**.*": "power-automate"
      },
      "ms.subservice": {
        "guidance/**.*": "guidance",
        "developer/**.*": "developer",
        "desktop-flows/**.*": "desktop-flow",
        "teams/**.*": "teams"
      }
    },
    "template": [],
    "dest": "power-automate",
    "markdownEngineName": "markdig"
  }
}
